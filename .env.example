# OpenAI API Configuration
# For GPT OSS 120B or OpenAI API
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_BASE_URL=http://localhost:8002/v1

# Wandb Configuration (Optional, for training monitoring)
WANDB_API_KEY=your-wandb-api-key-here
WANDB_PROJECT=llm-as-judge
WANDB_ENTITY=your-wandb-entity

# Model Paths
# Adjust these paths to match your local setup
QWEN_MODEL_PATH=/path/to/qwen2.5-7b-instruct
GPT_OSS_MODEL_PATH=/path/to/gpt-oss-120b

# AFlow Path
AFLOW_PATH=/path/to/AFlow

# GPU Configuration
CUDA_VISIBLE_DEVICES=0

# Training Configuration
PROTECTED_PIDS=

# Data Paths
DATA_DIR=./data
EXPERIENCE_BUFFER_DIR=./data/experience_buffer

# Logging
LOG_DIR=./logs
LOG_LEVEL=INFO
